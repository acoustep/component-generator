{
    "name": "acoustep/component-generator",
		"license": "MIT",
    "description": "Laravel 4 package for generating HTML components from Web Frameworks such as Twitter Bootstrap via command line.",
		"keywords": ["laravel", "generator", "bootstrap", "foundation", "component generator"],
    "authors": [
        {
            "name": "Mitch Stanley",
            "email": "mitch+componentgenerator@fastmail.co.uk"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.2.*",
				"illuminate/console": "4.2.*",
				"illuminate/filesystem": "4.2.*"
    },
		"require-dev": {
			"phpunit/phpunit": "4.1.*",
			"mockery/mockery": "dev-master"
		},
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "Acoustep\\ComponentGenerator\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
